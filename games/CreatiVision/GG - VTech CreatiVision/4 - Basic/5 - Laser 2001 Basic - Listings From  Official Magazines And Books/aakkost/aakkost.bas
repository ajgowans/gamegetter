10  INPUT "MONTAKO NIMEA SYOTAT";MAX
20  DIM SANA$(MAX)
30  FOR I = 1 TO MAX
40  INPUT "ANNA NIMI ";NIMI$
50 SANA$(I) = NIMI$
60  NEXT I
70 RAJA = MAX - 1
80 KIERROS = 0
85 VAIH = 0
90  FOR P = 1 TO RAJA
100  IF SANA$(P) <  = SANA$(P + 1) THEN 120
110 JEM$ = SANA$(P):SANA$(P) = SANA$(P + 1):SANA$(P + 1) = JEM$:VAIH = 1
120 KIERROS = KIERROS + 1
130  NEXT P
140 RAJA = RAJA - 1
150  IF VAIH = 1 THEN 85
160  FOR J = 1 TO MAX
170  PRINT ;SANA$(J);"  ";
180  NEXT J
